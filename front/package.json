{
  "name": "taiga-contrib-letschat",
  "version": "1.9.1",
  "description": "Taiga contrib plugin for LetsChat integration",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist || true",
    "create-dist": "mkdir -p dist",
    "copy-statics": "cp letschat.json dist/",
    "build-js": "{ cat 'coffee/letschat.coffee'; node_modules/jade/bin/jade.js < 'partials/letschat.jade'; } | node_modules/coffee-script/bin/coffee --compile --stdio > dist/letschat.js",
    "build": "npm run clean; npm run create-dist; npm run copy-statics; npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taigaio/taiga-contrib-letschat"
  },
  "keywords": [
    "letschat",
    "taiga",
    "integration"
  ],
  "author": "Andrea Stagi <stagi.andrea@gmail.com>",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/taigaio/taiga-contrib-letschat/issues"
  },
  "homepage": "https://github.com/taigaio/taiga-contrib-letschat",
  "dependencies": {
    "coffee-script": "^1.8.0",
    "jade": "^1.9.1"
  }
}
